{
  "name": "typescript-to-reverse",
  "version": "1.0.0",
  "description": "TypeScript development environment",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "dev:once": "ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^16.17.1",
    "nodemon": "^2.0.20",
    "prettier": "^3.6.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "express": "^4.18.1"
  }
}
